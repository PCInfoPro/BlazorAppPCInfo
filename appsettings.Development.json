{
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": true,
    "targets": {
      "file": {
        "type": "File",
        "fileName": "C:/Users/franm/Documents/Projects/logs/PCWeb_LogAS.log",
        "layout": "${longdate} ${level:uppercase=true} ${message} ${exception:format=toString,StackTrace}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "file"
      }
    ]
  }
}
